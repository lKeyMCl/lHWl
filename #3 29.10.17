/* Напишите программу находящую обратный по модулю элемент.
Иначе говоря по числам  X и N такое число Y, что остаток от деления X*Y на
N равняется единице. В программе гарантируется, что все числа - натуральные */

#include <iostream>

using namespace std;

int main()
{
    int x, n;
    cin >> x >> n;
    for (int i = 0;; ++i)
    {
        if ((x * i % n == 1) or (x * i == (n * (i - 1) + 1)))
        {
            cout << i;
            return 0;
        }
        if (x * i < n * (i - 1) + 1)
        {
            cout << "None";
            return 0;
        }
    }
}
