//Напишите программу, проверяющую N чисел на четность. В программе гарантируется, что все числа целые и их не менее одного
#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int A[n];
    for (int i = 0; i < n; ++i)
        cin >> A[i];
    for (int i = 0; i < n; ++i)
    {
        cout << A[i];
        if (A[i] % 2 == 0)
            cout << ": Even" << endl;
        else
            cout << ": Odd" << endl;
    }
    return 0;
}
